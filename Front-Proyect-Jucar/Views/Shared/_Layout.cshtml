<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Front_Proyect_Jucar</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Front_Proyect_Jucar.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.4/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link href="https://cdn.datatables.net/v/dt/dt-1.13.4/datatables.min.css" rel="stylesheet" />

</head>
<body>
    <body style="--bs-body-bg: var(--bs-gray-100);--bs-body-color: var(--bs-success);color: var(--bs-red);">
        <nav class="navbar navbar-light navbar-expand-md py-3" style="margin: 0 , 0 , 0 ;margin-top: 15px;color: var(--bs-secondary);background: var(--bs-danger);height: 150px;padding: 150px 50px;">
            <div class="container">
                <img src=~/Imagenes/logoJucar.jpg width="218" height="117" /><a class="navbar-brand d-flex align-items-center" href="#"></a><button class="navbar-toggler" data-bs-toggle="collapse" data-bs-target="#navcol-1"><span class="visually-hidden">Toggle navigation</span><span class="navbar-toggler-icon"></span></button>
                <div id="navcol-1" class="collapse navbar-collapse">
                    <ul class="navbar-nav ms-auto">
                        <li class="nav-item"></li>
                        <li class="nav-item"></li>
                        <li class="nav-item"></li>
                    </ul>
                </div><a asp-action=""></a><span style="text-align: center;padding-top: 0px;padding-bottom: 5px;margin-top: 2px;font-size: 30px;font-family: Alatsi, sans-serif;color: var(--bs-white);font-weight: bold;padding-left: 0px;padding-right: 55px;margin-left: 18px;margin-right: -24px;">AUTOPARTES JUCAR SAS </span>
            </div>
        </nav>
    </body>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdn.datatables.net/v/dt/dt-1.13.4/datatables.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
